#include <iostream>
#include <queue>
#include <stack>
#include <algorithm>
#include <map>
#include <string>
#include <math.h>
#include <unordered_map>
using namespace std;
typedef long long ll;

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int test;
	cin >> test;
	int cnt = 1;
	while (test--)
	{
		int max = 0;
		for (int i = 0; i < 5; i++)
		{
			int tmp;
			cin >> tmp;
			if (tmp > max)
				max = tmp;
		}
		cout << "Case #" << cnt++ << ": " << max << "\n";
	}
}
