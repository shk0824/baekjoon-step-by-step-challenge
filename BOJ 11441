#include <iostream>
#include <queue>
#include <string>
#include <iomanip>
#include <math.h>
using namespace std;
typedef long long ll;

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	int n,m;
	cin >> n;
	int*number = new int[n+1]();
	for (int i = 1; i <=n; i++)
		cin >> number[i],number[i]+=number[i-1];

	cin >> m;
	for (int i = 0; i < m; i++)
	{
		int a, b;
		cin >> a >> b;
		cout << number[b] - number[a - 1] << "\n";
	}

}
