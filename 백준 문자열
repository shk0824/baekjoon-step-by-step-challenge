n=int(input())


c=0
def checker():


    
    for j in w[1:]:


         if k[-1]!=j:

            if j in k:
                return False
            k.append(j)
    return True
    

for i in range(n):
    k=[1]

    w=input()
    k=k+[w[0]]
    
    if checker():

        c+=1
print(c)
