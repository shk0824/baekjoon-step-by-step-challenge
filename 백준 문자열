# print(s.index("1"))
L=[]

for i in "abcdefghijklmnopqrstuvwxyz":
    if i in s:
        L.append(s.index(i))
    else:
        L.append(-1)
for k in L:
    print(k,end=" ")

t=int(input())


for i in range(1,t+1):
    p=""
    r,s=input().split()
    r=int(r)
    for k in s:
        p+=r*k 
    print(p) 

s = input().upper()
b = list(set(s))
c=[]

for i in b:
    c.append(s.count(i))

if c.count(max(c))>=2:
    print("?")
else:
    print(b[c.index(max(c))])






