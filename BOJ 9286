#include <iostream>
#include <queue>
#include <stack>
#include <algorithm>
#include <map>
#include <string>
#include <math.h>
#include <unordered_map>
using namespace std;
typedef long long ll;

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	int test;
	cin >> test;
	int cnt = 1;
	while (test--)
	{
		int n;
		cin >> n;
		cout << "Case " << cnt++ << ": \n";
		while (n--)
		{
			int level;
			cin >> level;
			level += 1;
			if (level > 6)
				continue;
			cout << level << "\n";
		}
	}
}
